<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'>
    <div>
        <h1>Todo List (<span ui:field="count"/>)</h1>
        <ul ui:field="children" class="{style.list}">
        </ul>
        <a ui:field="addNew" href="#">Add New</a>
    </div>

    <ui:style>
        .list {
            list-style-type: none;
        }
    </ui:style>
</ui:UiBinder>